<p id="notice"><%= notice %></p>

<h1>Consulta</h1>

<table>
  <thead>
    <tr>
      <th>Paciente</th>
      <th>Sintomas</th>
      <th>Diagnostico</th>
      <th>Fecha llegada</th>
      <th>Fecha salida</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consulta.each do |consultum| %>
      <tr>
        <td><%= consultum.paciente %></td>
        <td><%= consultum.sintomas %></td>
        <td><%= consultum.diagnostico %></td>
        <td><%= consultum.fecha_llegada %></td>
        <td><%= consultum.fecha_salida %></td>
        <td><%= link_to 'Show', consultum %></td>
        <td><%= link_to 'Edit', edit_consultum_path(consultum) %></td>
        <td><%= link_to 'Destroy', consultum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Consultum', new_consultum_path %>
